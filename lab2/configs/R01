/ip pool
add name=dhcp_pool ranges=192.168.1.3-192.168.1.50
/ip dhcp-server
add address-pool=dhcp_pool disabled=no interface=ether5 name=dhcp
/ip address
add address=10.10.10.2/29 interface=ether3 network=10.10.10.0
add address=30.30.30.3/29 interface=ether4 network=30.30.30.0
add address=192.168.1.2/24 interface=ether5 network=192.168.1.0
/ip dhcp-server network
add address=192.168.1.0/24 dns-server=8.8.8.8,8.8.4.4 gateway=192.168.1.2
/ip route
add distance=1 dst-address=192.168.2.0/24 gateway=10.10.10.3
add distance=1 dst-address=192.168.3.0/24 gateway=30.30.30.2
/system identity
set name=R01_msk