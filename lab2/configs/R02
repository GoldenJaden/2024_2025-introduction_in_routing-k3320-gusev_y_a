/ip pool
add name=dhcp_pool ranges=192.168.2.3-192.168.2.50
/ip dhcp-server
add address-pool=dhcp_pool disabled=no interface=ether5 name=dhcp
/ip address
add address=10.10.10.3/29 interface=ether3 network=10.10.10.0
add address=20.20.20.2/29 interface=ether4 network=20.20.20.0
add address=192.168.2.2/24 interface=ether5 network=192.168.2.0
/ip dhcp-server network
add address=192.168.2.0/24 dns-server=8.8.8.8,8.8.4.4 gateway=192.168.2.2
/ip route
add distance=1 dst-address=192.168.1.0/24 gateway=10.10.10.2
add distance=1 dst-address=192.168.3.0/24 gateway=20.20.20.3
/system identity
set name=R02_brl