/ip pool
add name=dhcp_pool ranges=192.168.3.3-192.168.3.50
/ip dhcp-server
add address-pool=dhcp_pool disabled=no interface=ether5 name=dhcp
/ip address
add address=20.20.20.3/29 interface=ether3 network=20.20.20.0
add address=30.30.30.2/29 interface=ether4 network=30.30.30.0
add address=192.168.3.2/24 interface=ether5 network=192.168.3.0
/ip dhcp-server network
add address=192.168.3.0/24 dns-server=8.8.8.8,8.8.4.4 gateway=192.168.3.2
/ip route
add distance=1 dst-address=192.168.1.0/24 gateway=30.30.30.3
add distance=1 dst-address=192.168.2.0/24 gateway=20.20.20.2
/system identity
set name=R03_frt